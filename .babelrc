{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    ["babel-plugin-transform-import-meta", { "module": "ES6" }],
    [
      "@babel/plugin-syntax-import-attributes",
      {
        "deprecatedAssertSyntax": true
      }
    ]
  ]
}
